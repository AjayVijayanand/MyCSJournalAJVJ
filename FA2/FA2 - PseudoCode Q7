//Pseudocode to print maximum out of 3 user entered numbers (using nested if condition)


int Num1
int Num2
int Num3
print "This code is to print maximum out of 3 user entered numbers (using nested if condition)"
input "Enter Number 1: ", Num1
input "Enter Number 2: ", Num2
input "Enter Number 3: ", Num3
if(Num1 > Num2){
    if (Num1 > Num3){
        print "Number 1 is the Greatest"
    } else {
        print "Number 3 is the Greatest"
    }
} else {
    if (Num2 > Num3){
        print "Number 2 is the Greatest"
    } else {
        print "Number 3 is the Greatest"
    }
}